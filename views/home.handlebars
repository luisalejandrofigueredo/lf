<script>
      var mode="dark"
      if (sessionStorage.getItem('color')==='light'){
         var element= document.body;
         element.classList.toggle('light');
         mode='light'
      }    
      function changeMode(){
        var element= document.body;
          element.classList.toggle('light');
        if (mode==='dark')  {
          mode='light'
          const miEnlace=document.getElementById('conocimiento');
          miEnlace.href="http://luisfigueredo.ddns.net/conocimientos?modeColor=light";
          const miEnlaceTitulo=document.getElementById('titulos');
          miEnlaceTitulo.href="http://luisfigueredo.ddns.net/titulos?modeColor=light"
          sessionStorage.setItem('color','light')
        } else {
          mode='dark'
          const miEnlace=document.getElementById('conocimiento');
          const miEnlaceTitulo=document.getElementById('titulos');
          miEnlaceTitulo.href="http://luisfigueredo.ddns.net/titulos?modeColor=dark"
          miEnlace.href="http://luisfigueredo.ddns.net/conocimientos?modeColor=dark";
          sessionStorage.setItem('color','dark')
        }
      }
</script>