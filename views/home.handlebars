<script>
  let showMenu=false;
  {{#if (eqString modeColor "light")}}
    var element= document.body; element.classList.toggle('light')
  {{/if}}
  async function changeColor() { const response = await
  fetch("http://luisfigueredo.ddns.net/changeColor"); location.reload(); }
  function showColorMenu(){ if (showMenu===false) { showMenu=true; let
  element=document.getElementById("columnMenu")
  element.classList.replace("columnMenu","columnMenuVisible") } else{
  showMenu=false; let element=document.getElementById("columnMenu")
  element.classList.replace("columnMenuVisible","columnMenu") } }
</script>